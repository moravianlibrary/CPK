<table>
  <tbody>
  <? foreach($this->idps as $idp):
    $json = json_encode($idp);
    ?>
    <tr class="link idp-record" data-ng-click="fedLoginCtrl.login('<?=htmlentities($json)?>')">
      <td class="col-sm-3">
        <? if($idp['logo']):?>
          <img height='30' src="<?=$idp['logo']?>" alt="<?=$idp['name']?>">
        <? endif; ?>
      </td>
      <td>
        <a href="<?=$idp['href']?>"><?=$idp['name']?></a>
      </td>
    </tr>
  <? endforeach; ?>
  </tbody>
</table>